{
"env": {
  "browser": true,
  "node": true,
  "es6": true
},
"modules": true,
"ecmaFeatures": {
  "modules": true,
  "jsx": true,
  "forOf": true,
  "blockBindings": true,
  "arrowFunctions": true,
  "classes": true,
  "defaultParams": true,
  "destructuring": true,
  "generators": true,
  "restParams": true,
  "spread": true,
  "superInFunctions": true,
  "templateStrings": true,
  "experimentalObjectRestSpread": true
},
"globals": {
  "describe": true,
  "it": true,
  "sinon": true
},
"plugins": [
  "react",
  "babel",
  "no-console-log"
],
"parser": "babel-eslint",
"rules": {
  "indent": [
    2,
    2,
    {
      "SwitchCase": 1
    }
  ],
  "quotes": [
    2,
    "single"
  ],
  "jsx-quotes": [
    2,
    "prefer-double"
  ],
  "no-debugger": 1,
  "no-empty": 2,
  "no-extra-parens": 0,
  "no-extra-semi": 1,
  "no-func-assign": 1,
  "no-cond-assign": 1,
  "no-irregular-whitespace": 1,
  "no-negated-in-lhs": 1,
  "no-obj-calls": 1,
  "no-sparse-arrays": 2,
  "no-unreachable": 1,
  "use-isnan": 1,
  "valid-jsdoc": 0,
  "valid-typeof": 2,
  "no-unexpected-multiline": 2,
  "max-depth": [
    2,
    3
  ],
  "complexity": [
    2,
    4
  ],
  "max-nested-callbacks": [
    2,
    6
  ],
  "no-unused-vars": 2,
  "no-eval": 2,
  "no-alert": 2,
  "no-caller": 2,
  "no-else-return": 1,
  "no-eq-null": 2,
  "no-fallthrough": 2,
  "no-extra-bind": 2,
  "no-invalid-this": 2,
  "no-labels": 2,
  "no-lone-blocks": 2,
  "no-multi-spaces": [
    1,
    {
      "exceptions": {
        "ImportDeclaration": true
      }
    }
  ],
  "no-new": 2,
  "no-redeclare": 2,
  "no-return-assign": 2,
  "no-self-compare": 1,
  "no-throw-literal": 2,
  "no-unused-expressions": 1,
  "no-useless-call": 2,
  "no-useless-concat": 1,
  "no-void": 2,
  "no-warning-comments": [
    0,
    {
      "terms": [
        "monkey",
        "todo",
        "fix",
        "bug",
        "error"
      ],
      "location": "anywhere"
    }
  ],
  "vars-on-top": 1,
  "yoda": 2,
  "strict": [
    2,
    "global"
  ],
  "global-require": 2,
  "callback-return": [
    2,
    [
      "callback",
      "cb",
      "next"
    ]
  ],
  "no-new-require": 2,
  "array-bracket-spacing": [
    1,
    "never"
  ],
  "block-spacing": [
    1,
    "always"
  ],
  "brace-style": [
    1,
    "stroustrup",
    {
      "allowSingleLine": true
    }
  ],
  "comma-spacing": [
    1,
    {
      "before": false,
      "after": true
    }
  ],
  "comma-style": [
    1,
    "last"
  ],
  "computed-property-spacing": [
    1,
    "never"
  ],
  "eol-last": 1,
  "func-names": 1,
  "id-length": 2,
  "key-spacing": [
    1,
    {
      "beforeColon": false,
      "afterColon": true
    }
  ],
  "lines-around-comment": [
    1,
    {
      "beforeBlockComment": true,
      "afterBlockComment": false,
      "beforeLineComment": true,
      "allowBlockStart": true
    }
  ],
  "linebreak-style": 2,
  "new-parens": 2,
  "newline-after-var": [
    1,
    "always"
  ],
  "no-array-constructor": 1,
  "no-continue": 1,
  "no-lonely-if": 2,
  "no-mixed-spaces-and-tabs": 2,
  "no-multiple-empty-lines": [
    1,
    {
      "max": 2
    }
  ],
  "no-nested-ternary": 2,
  "no-negated-condition": 0,
  "no-new-object": 2,
  "no-spaced-func": 1,
  "no-trailing-spaces": [
    2,
    {
      "skipBlankLines": true
    }
  ],
  "no-underscore-dangle": 2,
  "no-unneeded-ternary": 1,
  "object-curly-spacing": [
    1,
    "always"
  ],
  "operator-assignment": [
    1,
    "always"
  ],
  "quote-props": [
    2,
    "consistent-as-needed"
  ],
  "require-jsdoc": 1,
  "semi-spacing": 2,
  "semi": [
    1,
    "always"
  ],
  "keyword-spacing": 1,
  "space-before-blocks": 1,
  "space-before-function-paren": [
    1,
    "never"
  ],
  "space-in-parens": [
    1,
    "never"
  ],
  "space-infix-ops": 2,
  "space-unary-ops": 1,
  "spaced-comment": [
    1,
    "always"
  ],
  "wrap-regex": 2,
  "arrow-parens": 2,
  "arrow-spacing": 2,
  "constructor-super": 2,
  "no-class-assign": 2,
  "no-const-assign": 2,
  "no-dupe-class-members": 2,
  "no-this-before-super": 2,
  "object-shorthand": 1,
  "prefer-arrow-callback": 2,
  "prefer-const": 0,
  "prefer-spread": 2,
  "prefer-template": 2,
  "react/display-name": 0,
  "react/jsx-no-undef": 1,
  "react/jsx-sort-props": 0,
  "react/jsx-uses-react": 1,
  "react/jsx-uses-vars": 1,
  "react/no-did-mount-set-state": 1,
  "react/no-did-update-set-state": 1,
  "react/no-multi-comp": 1,
  "react/no-unknown-property": 1,
  "react/prop-types": 1,
  "react/react-in-jsx-scope": 1,
  "react/self-closing-comp": 1,
  "react/wrap-multilines": 1,
  "babel/generator-star-spacing": 1,
  "babel/object-curly-spacing": 0,
  "babel/object-shorthand": 1,
  "no-console-log/no-console-log": 2
}
}
